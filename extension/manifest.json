{
    "manifest_version": 3,
    "name": "Ilias Exporter",
    "description": "Exports Ilias Questions",
    "version": "1.0",
    "action": {
        "default_popup": "default.html"
    },
    "permissions": ["activeTab", "storage", "tabs", "scripting"],
    "background": {
        "service_worker": "background.js"
    }
}
